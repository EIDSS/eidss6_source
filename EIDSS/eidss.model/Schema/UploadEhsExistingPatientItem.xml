      </readonly>
      <actions>
        <standard>
          <remove type="Create"/>
          <remove type="Delete"/>
          <remove type="SelectAll"/>
          <remove type="Ok"/>
          <remove type="Cancel"/>
          <remove type="Save"/>
        </standard>
      </actions>
